.area-staff
  width 960px
  margin 60px auto 20px
.box-staff
  width 300px
  display inline-block
  border-radius 15px
  height 75px
  margin 10px
  background-color #DFD6AE
  position relative
.program-staff
  // background-color #82AFF9
  &:before
    content url("../img/credit/programer-icon.png")
    transform scale(0.5)
    -webkit-transform scale(0.5)
    -ms-transform scale(0.5)
    -moz-transform scale(0.5)
    position absolute
.design-staff
  &:before
    content url("../img/credit/designer-icon.png")
    transform scale(0.5)
    -webkit-transform scale(0.5)
    -ms-transform scale(0.5)
    -moz-transform scale(0.5)
    position absolute
    right 68px
    top 18px
